  <h1>creditos:</h1>
  <div id="gameBoard">
    <!-- Cartas del juego -->
    <div class="card" style="background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQVM1bCRUYufRlplRblRwlmNcKUUctCKc3WyezfOIA1SgL-8eVVgvzUgPk&s=10')" onclick="revealCard(this)"></div>
    <div class="card" style="background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQqrpBwBoQ7N81kJlO-QQYxaPQhTBW3WIE0mg&usqp=CAU')" onclick="revealCard(this)"></div>
    <div class="card" style="background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSGxsq0k5YRDW6bxr3WhByytaC68YEOgKnPz9MPp-XWPQ&s')" onclick="revealCard(this)"></div>
    <div class="card" style="background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQtadkVx9JROZkDEyR4kqsZXUZm-EXyzRhKWXamAmqrvE3a1knAXSd2ugWC&s=10')" onclick="revealCard(this)"></div>
    <div class="card" style="background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSBiYJ0tiHpMXkOe_JsM3eu7y72HsZV78mi7Ho7fWsOyQ&s')" onclick="revealCard(this)"></div>
    <div class="card" style="background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQUc09Fha31HeyFyia_Xje57KIzO1GrelzyvyVjP_N8ESGASMwY7nAZZ3E&s=10')" onclick="revealCard(this)"></div>
  </div>

  <script>
    let firstCard = null;
    let secondCard = null;

    function revealCard(card) {
      // Evitar que se haga clic en cartas ya reveladas
      if (card.classList.contains('revealed')) {
        return;
      }
      
      card.style.backgroundImage = 'url(' + card.getAttribute('data-img') + ')';
      card.classList.add('revealed');

      // Si es la primera carta revelada, almacenarla
      if (firstCard === null) {
        firstCard = card;
      } else {
        // Si es la segunda carta revelada, comparar con la primera
        secondCard = card;
        checkMatch();
      }
    }

    function checkMatch() {
      // Si las cartas coinciden
      if (firstCard.getAttribute('data-img') === secondCard.getAttribute('data-img')) {
        // Mantener las cartas reveladas
        firstCard.classList.add('revealed');
        secondCard.classList.add('revealed');
      } else {
      setTimeout(() => {
          firstCard.style.backgroundImage = '';
          secondCard.style.backgroundImage = '';
        }, 1000);
      }

      // Restablecer las variables firstCard y secondCard
      firstCard = null;
      secondCard = null;
    }
